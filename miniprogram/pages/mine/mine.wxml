
<view class="header-img-container">
    <image class="header-img"
           mode="aspectFill"
           bind:longtap="changeHeaderImg"
           src="data:image/jpeg;base64,"></image>
</view>
<view class="profile-header">
    <navigation-bar back="{{true}}" color="#fff" />
    <view class="avatar-url">
        <open-data type="userAvatarUrl"></open-data>
    </view>
    <open-data type="userNickName" class="nick-name"/>
</view>

<view class="nav">
    <view class="nav-item">
        <view class="content" hover-class="none" bind:tap="onGetQRCode">
            <text class="iconfont icon-ziyuanldpi"></text>
            <text class="text">小程序码</text>
            <text class="iconfont icon-xiangyou"></text>
        </view>
    </view>

</view>
